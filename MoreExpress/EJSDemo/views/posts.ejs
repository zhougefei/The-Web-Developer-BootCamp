<%- include("partials/header") %>
<h1>The Posts Page!<h1>
<% for (var i = 0; i < posts.length; i++) { %>
	<li>
		<%= posts[i].title %> - <strong><%= posts[i].author %></strong>
	</li>
<% } %>
<% posts.forEach(function(po){ %>
	<li>
		<%= po.title %> - <strong><%= po.author %></strong>
	</li>	
<% }) %>
	<%- include("partials/footer") %>